  "description": "",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "connect-redis": "^4.0.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.7.13",
